public class Solution2_2 {

    public static void main(String[] args) {
        int i ;
        System.out.println("  *  ");
        for (i=0 ;i<2 ;i++) {
            System.out.print(" *");
        }
        System.out.println("\r");
        for (i=0;i<3;i++){
            System.out.print("* ");
        }
        }
    }
